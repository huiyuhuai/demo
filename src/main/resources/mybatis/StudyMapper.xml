<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.dao.InformationDao">

    <!--表information-->
    <select id="listInformation" resultType="com.example.demo.domain.InformationDO">
        SELECT * FROM information where NAME='惠'
    </select>

    <!--表student-->
    <select id="listStudent" resultType="com.example.demo.domain.StudentDO">
        select  * from  studentStudyMapper.xml
    </select>
</mapper>
